<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">

    <record id="base.group_document_technical_manager" model="res.groups">
        <field name="name">Technical Manager</field>
        <field name="category_id" ref="knowledge.module_category_knowledge"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

    <record id="view_documents_by_group" model="ir.rule">
        <field name="name">Users have access to documents based on their group permissions.</field>
        <field name="model_id" ref="model_document_page"/>
        <field eval="True" name="global"/>
        <field name="sequence" eval="1" />
        <field name="domain_force">['|', ('groups_id', 'in', user.groups_id.ids), ('groups_id', '=', False)]</field>
        <!--<field eval="[(4,ref('hr.group_hr_manager'))]" name="groups"/>-->
    </record>

</data>
</odoo>
